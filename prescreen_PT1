// Si sélectionnent “deux roues”
var deux_roues_mot = {
  type: jsPsychSurveyMultiSelect,
  questions: [
    {
      prompt: "Quand avez-vous commencé à conduire votre deux roues motorisé ?", 
      options: ["Il y a moins d’un mois",
                "Il y a 1-3 mois", 
